{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "main.dart.js", "use": "@vercel/static" },
    { "src": "flutter_service_worker.js", "use": "@vercel/static" },
    { "src": "assets/**", "use": "@vercel/static" },
    { "src": "manifest.json", "use": "@vercel/static" },
    { "src": "version.json", "use": "@vercel/static" },
    { "src": "icons/**", "use": "@vercel/static" },
    { "src": "flutter.js", "use": "@vercel/static" },
    { "src": "canvaskit/**", "use": "@vercel/static" },
    { "src": "flutter_bootstrap.js", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/(.*\\.js|.*\\.json|.*\\.wasm|.*\\.html|.*\\.png|.*\\.svg|.*\\.css|.*\\.ico|.*\\.txt)$",
      "dest": "/$1"
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
